{
  "name": "iselectenergy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compileTypeScriptFiles": "rimraf ./tests_output/ && rimraf ./screens/ && rimraf chromedriver.log && cd ./framework/test/Regression && tsc --outDir ../../build",
    "runTestsOfRegression:uat": "npx nightwatch ./framework/build/test/Regression --reporter html-reporter.js --config=./framework/config/nightwatch.conf.js --env uat",
    "runTestsOfRegression:prod": "npx nightwatch ./framework/build/test/Regression --reporter html-reporter.js --config=./framework/config/nightwatch.conf.js --env prod",

    "runSingleTestOfRegression:uat": "npx nightwatch ./framework/build/test/Regression/YourUsagePageTest.js --reporter html-reporter.js --config=./framework/config/nightwatch.conf.js --env uat",

    "allureReport": "allure generate ./allure-results --clean && allure open"
  },
  "author": "Peter (Kumar Raja Yerupalli)",
  "license": "ISC",
  "dependencies": {
    "@types/nightwatch": "^1.3.4",
    "@types/node": "^15.14.9",
    "nightwatch": "^1.7.11",
    "nightwatch-html-reporter": "^2.0.6"
  },
  "devDependencies": {
    "chromedriver": "^97.0.0",
    "fs": "0.0.1-security",
    "geckodriver": "^2.0.4",
    "handlebars": "^4.7.7",
    "nightwatch-vrt": "^0.2.10",
    "nightwatch-allure": "^1.2.0",
    "nyc": "^15.1.0",
    "path": "^0.12.7",
    "typescript": "^4.4.4"
  }
}
